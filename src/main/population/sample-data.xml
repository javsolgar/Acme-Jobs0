<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="conversion-service.xml" />
	
	<!--  Shouts -->
	
	<bean id="shout01" class="acme.entities.shouts.Shout">
	<property name="author" value="John Doe"></property>
	<property name="text" value="I'm happy"></property>
	<property name="moment" value="2019/09/09 10:00"></property>
	</bean>
	
	<bean id="shout02" class="acme.entities.shouts.Shout">
	<property name="author" value="Jane Doe"></property>
	<property name="text" value="I'm the master of my room!"></property>
	<property name="moment" value="2019/08/09 15:55"></property>
	</bean>
	
	<bean id="shout03" class="acme.entities.shouts.Shout">
	<property name="author" value="Foo Bar"></property>
	<property name="text" value="Just turning 20!"></property>
	<property name="moment" value="2019/09/07 00:00"></property>
	</bean>
	
	<!--  GonzalezBulletin -->
	
	<bean id="Gonzalez01" class="acme.entities.bulletins.Gonzalez">
	<property name="author" value="John Doe"></property>
	<property name="text" value="I'm happy"></property>
	<property name="city" value="Seville"></property>
	<property name="moment" value="2019/09/09 10:00"></property>
	</bean>
	
	<bean id="Gonzalez02" class="acme.entities.bulletins.Gonzalez">
	<property name="author" value="Jane Doe"></property>
	<property name="text" value="I'm the master of my room!"></property>
	<property name="city" value="Barcelona"></property>
	<property name="moment" value="2019/08/09 15:55"></property>
	</bean>
	
	<bean id="Gonzalez03" class="acme.entities.bulletins.Gonzalez">
	<property name="author" value="Foo Bar"></property>
	<property name="text" value="Just turning 20!"></property>
	<property name="city" value="Madrid"></property>
	<property name="moment" value="2019/09/07 00:00"></property>
	</bean>
	
	
	<!--  SolisBulletin -->
	<bean id="SolisBulletin01" class="acme.entities.bulletins.SolisBulletin">
	<property name="author" value="Mick Wallas"></property>
	<property name="state" value="Nervous"></property>
	<property name="text" value="I'm gonna have my last exam of ADDA"></property>
	<property name="moment" value="2019/04/02 13:00"></property>
	</bean>
	
	<bean id="SolisBulletin02" class="acme.entities.bulletins.SolisBulletin">
	<property name="author" value="Jack Butcher"></property>
	<property name="state" value="Angry"></property>
	<property name="text" value="Someone have just stolen my wallet "></property>
	<property name="moment" value="2019/05/06 17:00"></property>
	</bean>
	
	<bean id="SolisBulletin03" class="acme.entities.bulletins.SolisBulletin">
	<property name="author" value="Javier Solís García"></property>
	<property name="state" value="Happy"></property>
	<property name="text" value="Today is my birthday!!"></property>
	<property name="moment" value="2019/09/03 09:02"></property>
	</bean>

</beans>
